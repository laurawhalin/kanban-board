<div class="unassigned">
  {{#each todo in unassigned}}
  <div class="cards">
    <span class="todo-project">{{todo.project}}</span>
  </div>
  {{/each}}
</div>

<div class="assigned">
  {{#each todo in assigned}}
  <div class="cards">
    <span class="todo-project">{{todo.project}}</span>
    <span class="todo-status">{{todo.status}}</span>
  </div>
  {{/each}}
</div>
